{
    "name": "Book Library Scanner",
    "version": "1.0",
    "slug": "book_library_scanner",
    "description": "Scan barcodes using your phone's camera and add books to a library.",
    "arch": [
        "armv7",
        "arm64",
        "amd64"
    ],
    "startup": "application",
    "boot": "auto",
    "host_network": true,
    "webui": "http://[HOST]:[PORT:5000]",
    "options": {
        "port": 5000
    },
    "schema": {
        "port": "int"
    }
}